<div class="nested-fields">

  <div class="input-field col s12 m6">
    <%= f.text_field :status %>
    <%= f.label :status %>
  </div>

  <div class="input-field col s12 m6">
    <%= f.text_field :status_date, class: "datepicker" %>
    <%= f.label :status_date, "Status Date" %>
  </div>

  <%= link_to_remove_association 'Remove status...', f %>

</div>
